<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Proyecto" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Colores de la Toolbar y demÃ¡s componentes -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorSecondary">@color/colorSecondary</item>
        <item name="colorOnPrimary">@color/colorOnPrimary</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="MaterialAlertDialog.Proyecto" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="colorSurface">@color/colorSurface</item>
        <item name="cornerSize">16dp</item>
    </style>
    <style name="ButtonStyle.Proyecto" parent="Widget.AppCompat.Button">
        <item name="android:backgroundTint">@color/colorSecondary</item>
        <item name="android:textColor">@color/colorOnPrimary</item>
        <item name="android:radius">16dp</item>
        <item name="android:padding">10dp</item>
    </style>
</resources>